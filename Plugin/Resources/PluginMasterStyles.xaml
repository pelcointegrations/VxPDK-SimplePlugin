    <!-- Default Image button style -->
    <Style TargetType="{x:Type ctrls:HoverImageButton}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="14px" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="{StaticResource BorderColor1Brush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ctrls:HoverImageButton}">
                    <Border Background="{TemplateBinding Background}" x:Name="PART_border" BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Image x:Name="PART_image"
                               HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                               Margin="{TemplateBinding Padding}"
                               VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                               Source="{TemplateBinding NormalImage}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <!-- Change the cursor when mouse over. -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver" Value="True" />
                                <Condition Property="IsEnabled" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter Property="Cursor" Value="Hand"/>
                        </MultiTrigger>
                        <!-- Change the image when mouse over. -->
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsMouseOver, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding IsEnabled, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding HoverImage, RelativeSource={RelativeSource Self}, Converter={StaticResource isNull}}" Value="False" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="PART_image" Property="Source" Value="{Binding HoverImage, RelativeSource={RelativeSource AncestorType={x:Type ctrls:HoverImageButton}}}" />
                        </MultiDataTrigger>
                        <!-- Make the button look disabled using opacity if DisabledImage is null. -->
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsEnabled, RelativeSource={RelativeSource Self}}" Value="False" />
                                <Condition Binding="{Binding DisabledImage, RelativeSource={RelativeSource Self}, Converter={StaticResource isNull}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="PART_border" Property="Opacity" Value=".5" />
                        </MultiDataTrigger>
                        <!-- Make the button look disabled using DisabledImage. -->
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsEnabled, RelativeSource={RelativeSource Self}}" Value="False" />
                                <Condition Binding="{Binding DisabledImage, RelativeSource={RelativeSource Self}, Converter={StaticResource isNull}}" Value="False" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="PART_image" Property="Source" Value="{Binding DisabledImage, RelativeSource={RelativeSource AncestorType={x:Type ctrls:HoverImageButton}}}" />
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>